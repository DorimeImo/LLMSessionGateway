{
  "ConnectionStrings": {
    "Redis": "localhost:6379",
    "AzureBlob": "UseDevelopmentStorage=true",
    "GrpcChatService": "http://localhost:5001"
  },

  "Redis": {
    "LockTtlSeconds": 15,
    "ActiveSessionTtlSeconds": 1800
  },

  "AzureBlob": {
    "ContainerName": "archivedsessions"
  },

  "Grpc": {
  },

  "Logging": {
    "File": {
      "BasePath": "Logs",
      "FileNamePattern": "{Year}/{Month}/{Day}-{Hour}-{Minute}-log.txt",
      "RollingInterval": "Day"
    }
  },

  "OpenTelemetry": {
    "Jaeger": {
      "AgentHost": "localhost",
      "AgentPort": 6831
    }
  }
}
